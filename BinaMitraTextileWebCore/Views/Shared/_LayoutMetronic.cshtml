@{
    Layout = null;
    ViewBag.Title = @Helper.COMPANYNAME;
}

<!DOCTYPE html>
<html lang="en">
<!--begin::Head-->
<head>
    <partial name='~/Views/Shared/Scripts_Header.cshtml' />
</head>
<!--end::Head-->

<!--begin::Body-->
<body id="kt_app_body"
      data-kt-app-page-loading-enabled="true"
      data-kt-app-page-loading="on"
      data-kt-app-layout="dark-sidebar"
      data-kt-app-header-fixed="true"
      data-kt-app-sidebar-enabled="true"
      data-kt-app-sidebar-fixed="true"
      data-kt-app-sidebar-minimize="on"
      data-kt-app-sidebar-hoverable="true"
      data-kt-app-sidebar-push-header="true"
      data-kt-app-sidebar-push-toolbar="true"
      data-kt-app-sidebar-push-footer="true" class="app-default">

    <partial name='~/Views/Shared/Partials/theme-mode/_init.cshtml' />
    <!--begin::App-->
    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page  flex-column flex-column-fluid " id="kt_app_page">
            <partial name='~/Views/Shared/Layout/Partials/_header.cshtml' />
            <!--begin::Wrapper-->
            <div class="app-wrapper  flex-column flex-row-fluid " id="kt_app_wrapper">
                <partial name='~/Views/Shared/Layout/Partials/_sidebar.cshtml' />
                <!--begin::Main-->
                <div class="app-main flex-column flex-row-fluid " id="kt_app_main">
                    <!--begin::Content wrapper-->
                    <div class="d-flex flex-column flex-column-fluid">
                        @* <partial name='~/Views/Shared/Layout/Partials/_content.cshtml' /> *@

                        <!--begin::Content-->
                        <div id="kt_app_content" class="app-content flex-column-fluid pt-0">
                            @RenderBody()
                        </div>
                        <!--end::Content-->
                    </div>
                    <!--end::Content wrapper-->
                    @* <partial name='~/Views/Shared/Layout/Partials/_footer.cshtml' /> *@
                </div>
                <!--end:::Main-->
            </div>
            <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->
    <partial name='~/Views/Shared/Partials/_drawers.cshtml' />
    <partial name='~/Views/Shared/Partials/_scrolltop.cshtml' />

    <partial name='~/Views/Shared/Scripts_Body.cshtml' />
    @RenderSection("inViewScripts", required: false)
</body>
<!--end::Body-->

</html>